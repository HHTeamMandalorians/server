{
  "extends": "@tsconfig/node16-strictest/tsconfig.json",
  "compilerOptions": {
    /* Projects */
    "incremental": true,
    "tsBuildInfoFile": "./cache/tsbuildinfo.json",
    /* Language and Environment */
    "target": "ES2022",
    /* Modules */
    "module": "Node16",
    /* Emit */
    "outDir": "./dist",
    "sourceMap": true,
    "removeComments": true,
    "importHelpers": true,
    "importsNotUsedAsValues": "error",
    "newLine": "lf",
    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    "skipLibCheck": true,
    "noEmitOnError": true
  }
}